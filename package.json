{
  "name": "dotfiles",
  "version": "1.1.0",
  "description": "Personal dotfiles to kickstart shell and development settings",
  "main": "lib/main.js",
  "bin": {
    "dot": "./bin/dot"
  },
  "engines": {
    "node": ">6.0.0",
    "npm": "~3.0.0"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "health-check": "npm outdated && npm run lint && npm test",
    "postinstall": "npm run build",
    "build": "npm run clean && babel src -d lib -s",
    "lint": "eslint ./src && eslint ./test",
    "test": "tape -r babel-register ./test/*.js"
  },
  "author": "George JOR",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhjor1212/dotfiles.git"
  },
  "keywords": [
    "alias",
    "config",
    "git",
    "zsh",
    "dotfiles"
  ],
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-eslint": "^6.0.2",
    "babel-preset-es2015": "^6.3.13",
    "common-errors": "^1.0.0",
    "eslint": "^3.0.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-plugin-import": "^1.10.2",
    "inquirer": "^1.1.1",
    "js-yaml": "^3.5.2",
    "logatim": "^0.9.0",
    "shelljs": "^0.7.0",
    "tape": "^4.4.0"
  }
}

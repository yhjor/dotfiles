{
  "name": "dotfiles",
  "version": "1.1.0",
  "description": "Personal dotfiles to kickstart shell and development settings",
  "main": "lib/main.js",
  "bin": {
    "dot": "./bin/dot"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "rimraf lib && babel src --out-dir lib --source-maps",
    "health": "npm outdated && npm run lint && npm test",
    "lint": "eslint --fix src && eslint --fix test",
    "test": "tape -r babel-register ./test/*.js"
  },
  "author": "George JOR",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yhjor1212/dotfiles.git"
  },
  "keywords": [
    "alias",
    "config",
    "git",
    "zsh",
    "dotfiles"
  ],
  "babel": {
    "presets": [
      "es2016"
    ]
  },
  "eslintConfig": {
    "extends": "eslint-config-airbnb-base",
    "parser": "babel-eslint",
    "env": {
      "node": true
    },
    "rules": {
      "semi": 0
    }
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-es2016": "^6.16.0",
    "common-errors": "^1.0.0",
    "inquirer": "^2.0.0",
    "lodash": "^4.17.4",
    "logatim": "^0.9.0",
    "rimraf": "^2.5.4",
    "shelljs": "^0.7.6"
  },
  "devDependencies": {
    "babel-eslint": "^7.1.1",
    "eslint": "^3.13.1",
    "eslint-config-airbnb-base": "^11.0.1",
    "eslint-plugin-import": "^2.2.0",
    "tape": "^4.6.3"
  }
}
